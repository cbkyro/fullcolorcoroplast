{"components/carousel":"<section class=\"heroCarousel\"\n    data-slick='{\n        \"dots\": true,\n        \"mobileFirst\": true,\n        \"slidesToShow\": 1,\n        \"slidesToScroll\": 1,\n        \"autoplay\": true,\n        \"autoplaySpeed\": {{carousel.swap_frequency}},\n        \"lazyLoad\": \"anticipated\"\n    }'\n    {{#if theme_settings.carousel_video_mute}} data-youtube-mute{{/if}}>\n    {{#each carousel.slides}}\n    <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_carousel_style '==' 'stretch'}}heroCarousel-slide--stretch{{/if}} heroCarousel-slide--{{@index}}\"\n        style=\"background-image: url({{image}})\">\n        <a href=\"{{url}}\">\n            {{#if @index '==' 0}}\n                {{#if (merge this video=../../theme_settings.carousel_vid0)}}{{/if}}\n            {{/if}}\n            {{#if @index '==' 1}}\n                {{#if (merge this video=../../theme_settings.carousel_vid1)}}{{/if}}\n            {{/if}}\n            {{#if @index '==' 2}}\n                {{#if (merge this video=../../theme_settings.carousel_vid2)}}{{/if}}\n            {{/if}}\n            {{#if @index '==' 3}}\n                {{#if (merge this video=../../theme_settings.carousel_vid3)}}{{/if}}\n            {{/if}}\n            {{#if @index '==' 4}}\n                {{#if (merge this video=../../theme_settings.carousel_vid4)}}{{/if}}\n            {{/if}}\n\n            {{#if video}}\n                <div class=\"heroCarousel-video-wrapper\">\n                    <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\" {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n                    <div class=\"heroCarousel-video\"><div data-youtube=\"{{video}}\"></div></div>\n                </div>\n            {{else}}\n                {{#if @index '==' 0}}\n                    <img class=\"heroCarousel-image\" src=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\" {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n                {{else}}\n                    <img class=\"heroCarousel-image\" data-lazy=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\" {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n                {{/if}}\n            {{/if}}\n            \n            {{#if heading}}\n                {{> components/carousel-content show_background=true}}\n            {{else}}\n                {{#if text}}\n                    {{> components/carousel-content show_background=true}}\n                {{else}}\n                    {{#if button_text}}\n                        {{> components/carousel-content show_background=false}}\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        </a>\n    </div>\n    {{/each}}\n</section>\n","components/carousel-content":"<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description\">{{{text}}}</p>\n    {{#if button_text}}\n        <span class=\"heroCarousel-action button button--primary button--large\">{{{button_text}}}</span>\n    {{/if}}\n</div>\n"}
