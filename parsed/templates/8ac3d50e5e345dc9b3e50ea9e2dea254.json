{"components/common/navigation-list-child":"{{#if children}}\n    <a\n        class=\"navPage-childList-action navPages-action has-subMenu\" \n        href=\"{{url}}\"\n        {{#if id '==' category_id}}data-current-category{{/if}}\n        data-collapsible=\"navPages-{{id}}{{idSuffix}}\"\n        {{#unless theme_settings.subcategories_menu_vertical}}data-collapsible-disabled-breakpoint=\"medium\"{{/unless}}\n        data-collapsible-disabled-state=\"open\"\n        data-collapsible-enabled-state=\"closed\">\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    </a>\n    <ul class=\"navPage-childList\" id=\"navPages-{{id}}{{idSuffix}}\">\n        <li class=\"navPage-childList-item\">\n            <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-childList-item\">\n                {{> components/common/navigation-list-child \n                    category_id=../category_id \n                    idSuffix=../idSuffix \n                    theme_settings=../theme_settings}}\n            </li>\n        {{/each}}\n    </ul>\n{{else}}\n    <a class=\"navPage-childList-action navPages-action\" href=\"{{url}}\" {{#if id '==' category_id}}data-current-category{{/if}}>{{name}}</a>\n{{/if}}\n"}
