{"components/emthemes-modez/category/ajax-products-by-category-sorting-tabs":"{{!--\nArguments:\n    - category_id (number)\n    - style (string): CSS class styling suffix\n    - style2 (string): CSS class styling suffix\n\n\nGlobal variables:\n    - categories (array)\n\n--}}\n\n{{#each categories}}\n    {{#if id '==' ../category_id}}\n        <div class=\"emthemesModez-productsByCategoryTabs emthemesModez-productsByCategoryTabs--sorting\n                {{#if ../../style}}emthemesModez-productsByCategoryTabs--{{../../../style}}{{/if}}\n                {{#if ../../style2}}emthemesModez-productsByCategoryTabs--{{../../../style2}}{{/if}}\n            \">\n            <ul class=\"tabs\" data-tab data-tab-emthemesmodez-products-by-category-tabs role=\"tablist\">\n                <li class=\"li-heading\"><h2 class=\"page-heading\"><a href=\"{{url}}\">{{name}}</a></h2></li>\n                <li class=\"tab is-active\" role=\"presentation\"><a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-featured-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"tab-productsByCategoryTabs-featured-{{id}}\">{{lang 'emthemesmodez.products_by_category_sorting_tabs.featured' }}</a></li>\n                <li class=\"tab\" role=\"presentation\"><a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-new-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-new-{{id}}\">{{lang 'emthemesmodez.products_by_category_sorting_tabs.new' }}</a></li>\n                <li class=\"tab \" role=\"presentation\"><a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-bestselling-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-bestselling-{{id}}\">{{lang 'emthemesmodez.products_by_category_sorting_tabs.bestselling' }}</a></li>\n                <li class=\"tab\" role=\"presentation\"><a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-reviews-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-reviews-{{id}}\">{{lang 'emthemesmodez.products_by_category_sorting_tabs.reviews' }}</a></li>\n                <li class=\"tab tab--link\" role=\"presentation\"><span><a class=\"tab-title\" href=\"{{url}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\">{{lang 'emthemesmodez.products_by_category_sorting_tabs.view_all'}}</a></span></li>\n            </ul>\n            <div class=\"tabs-contents\">\n\n                <section role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content is-active\" id=\"tab-productsByCategoryTabs-featured-{{id}}\" \n                    data-emthemesmodez-products-by-category-tabs=\"{{concat url '?sort=featured'}}\">\n                    {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\n                </section>\n\n                <section role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-new-{{id}}\" \n                    data-emthemesmodez-products-by-category-tabs=\"{{concat url '?sort=newest'}}\">\n                    {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\n                </section>\n\n                <section role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-bestselling-{{id}}\" \n                    data-emthemesmodez-products-by-category-tabs=\"{{concat url '?sort=bestselling'}}\">\n                    {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\n                </section>\n\n                <section role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-reviews-{{id}}\" \n                    data-emthemesmodez-products-by-category-tabs=\"{{concat url '?sort=avgcustomerreview'}}\">\n                    {{> components/emthemes-modez/common/product-loading theme_settings=../../theme_settings}}\n                </section>\n            </div>\n        </div>\n    {{/if}}\n{{/each}}\n\n","components/emthemes-modez/common/product-loading":"<div class=\"emthemesModez-productLoading\">\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n\t<div class=\"emthemesModez-productLoading-item\"><img src=\"{{cdn theme_settings.image_product_loading}}\" alt=\"loading\" title=\"loading\" /></div>\n</div>","components/emthemes-modez/sections/section-products-by-category-sorting-tabs":"{{!--\nArguments:\n\t- index (number): section instance index number\n--}}\n\n{{> components/emthemes-modez/category/ajax-products-by-category-sorting-tabs\n    category_id=(lang (concat \"emthemesmodez.products_by_category_sorting_tabs.category_id_\" index))\n    style='supermarket1'\n    style2=(concat 'instance' index)\n}}\n\n"}
