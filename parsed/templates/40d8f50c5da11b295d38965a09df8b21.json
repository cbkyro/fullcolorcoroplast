{"components/common/body":"<div class=\"body\">\n    {{#block \"hero\"}} {{/block}}\n    <div class=\"container\">\n        {{#block \"page\"}} {{/block}}\n    </div>\n    {{> components/common/modal}}\n</div>\n\n<div class=\"scrollToTop\">\n    <a href=\"#topOfPage\" class=\"button u-hiddenVisually\" id=\"scrollToTopFloatingButton\" title=\"{{lang 'common.top_of_page'}}\"><i class=\"fa fa-chevron-up\"></i></a>\n</div>","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n\n<div id=\"previewModal\" class=\"modal modal--large\" data-reveal>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n"}
